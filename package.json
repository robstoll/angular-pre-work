  "bugs": {
    "url": "https://github.com/robstoll/angular-pre-work/issues"
  },
  "homepage": "https://github.com/robstoll/angular-pre-work#readme",
  "dependencies": {},
  "devDependencies": {
    "bower": "^1.6.3",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "uglify-js": "^2.5.0"
  },
  "scripts": {
    "postinstall": "bower install && npm run dist",
